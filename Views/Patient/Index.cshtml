@model IEnumerable<iCareWebApplication.Models.Patient>

@{
    ViewData["Title"] = "Patient List";
}

<h2>Role List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Patient ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.PatientId</td>
            </tr>
        }
    </tbody>
</table>

<p>
    <a asp-action="Create" class="btn btn-primary">Add New Patient</a>
</p>
